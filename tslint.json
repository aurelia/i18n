{
  "extends": [
    "tslint:recommended"
  ],
  "rules": {
    "quotemark": [
      true,
      "double"
    ],
    "object-literal-sort-keys": false,
    "ordered-imports": [
      false
    ],
    "array-type": false,
    "whitespace": [
      false,
      "check-branch",
      "check-decl",
      "check-operator",
      "check-module",
      "check-separator",
      "check-type"
    ],
    "interface-name": [
      true,
      "never-prefix"
    ],
    "no-shadowed-variable": false,
    "no-string-literal": false,
    "trailing-comma": false
  }
}
