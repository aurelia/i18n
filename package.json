    "gulp-typedoc-extractor": "0.0.8",
    "jasmine-core": "^2.1.3",
    "karma": "^0.13.15",
    "karma-babel-preprocessor": "^5.2.2",
    "karma-chrome-launcher": "^0.1.7",
    "karma-coverage": "^0.5.3",
    "karma-jasmine": "^0.3.5",
    "karma-jasmine-ajax": "^0.1.13",
    "karma-jspm": "^2.0.1",
