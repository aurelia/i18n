    "aurelia-templating-resources": "^1.0.0-beta.1.0.2"
  },
  "devDependencies": {
    "aurelia-tools": "^0.1.15",
    "babel": "^5.8.23",
    "babel-dts-generator": "^0.2.17",
    "babel-eslint": "^4.1.1",
    "conventional-changelog": "0.0.11",
    "del": "^1.1.0",
